{
  "name": "hyperlocal-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "cross-env NODE_ENV=test yarn test:unit && cross-env NODE_ENV=test yarn test:e2e",
    "test:e2e": "cross-env NODE_ENV=test jest --testMatch '<rootDir>/app/api/**/*.test.ts'",
    "test:unit": "cross-env NODE_ENV=test jest --testMatch '<rootDir>/app/_lib/server/models/**/*.test.ts'",
    "db:drop": "npx ts-node --skip-project ./scripts/drop-db.ts",
    "db:seed": "npx ts-node --skip-project ./scripts/seed-data.ts",
    "docs": "docker run -p 80:8080 -e SWAGGER_JSON=/docs/openapi.json -v ~/code/hyperlocal-nextjs/docs:/docs swaggerapi/swagger-editor",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@metamask/providers": "^14.0.2",
    "babel-jest": "^29.7.0",
    "ethers": "^6.10.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.3",
    "slugify": "^1.6.6",
    "styled-components": "^6.1.8",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@faker-js/faker": "^8.3.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.3",
    "node-mocks-http": "^1.14.0",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
